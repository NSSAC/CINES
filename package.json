    },
    "scripts": {
        "start": "yarn run tailwind && react-scripts start",
        "build": "yarn run tailwind && node ./purge-tailwindcss.js && cross-env GENERATE_SOURCEMAP=false react-scripts build",
        "tailwind": "./node_modules/.bin/tailwind build ./src/styles/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
        "test": "react-scripts test --env=node",
        "eject": "react-scripts eject",
        "build-docs": "find ./src/app/main/documentation/material-ui-components \\( -name '*.ts' -o -name '*.tsx' \\) -type f -delete && babel-node src/app/main/documentation/material-ui-components/build.js",
        "build-stats": "yarn run build --stats",
        "bundle-report": "yarn run build-stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
        "tarball-prod": "cp .env.cines_prod .env.production; yarn run build && rm cines-build-prod.tar; npm run build && tar -cvf cines-build-prod.tar build/ ; rm .env.production",
        "tarball-alpha": "cp .env.alpha .env.production && cat .env && yarn run build && rm cines-build-alpha.tgz; npm run build && tar -czvf cines-build-alpha.tgz build/ ; rm .env.production",
        "tarball-ptest": "cp .env.ptest .env.production && cat .env && yarn run build && rm cines-build-ptest.tgz; npm run build && tar -czvf cines-build-ptest.tgz build/ ; rm .env.production"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    },
    "homepage": "."
}
